  1116                          hexascii:
  1117   0008b3 57                      LD      d,a             ; save A in D
  1118   0008b4 cdc208                  CALL    hexconv         ; convert low nibble of A to hex
  1119   0008b7 5f                      LD      e,a             ; save it in E
  1120   0008b8 7a                      LD      a,d             ; get original value back
  1121   0008b9 07                      RLCA                    ; rotate high order nibble to low bits
  1122   0008ba 07                      RLCA
  1123   0008bb 07                      RLCA
  1124   0008bc 07                      RLCA
  1125   0008bd cdc208                  CALL    hexconv         ; convert nibble
  1126   0008c0 57                      LD      d,a             ; save it in D
  1127   0008c1 c9                      RET                     ; done
