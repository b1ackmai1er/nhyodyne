  1124                          hexascii:
  1125   0008c5 57                      LD      d,a             ; save A in D
  1126   0008c6 cdd408                  CALL    hexconv         ; convert low nibble of A to hex
  1127   0008c9 5f                      LD      e,a             ; save it in E
  1128   0008ca 7a                      LD      a,d             ; get original value back
  1129   0008cb 07                      RLCA                    ; rotate high order nibble to low bits
  1130   0008cc 07                      RLCA
  1131   0008cd 07                      RLCA
  1132   0008ce 07                      RLCA
  1133   0008cf cdd408                  CALL    hexconv         ; convert nibble
  1134   0008d2 57                      LD      d,a             ; save it in D
  1135   0008d3 c9                      RET                     ; done
