00DA1C  3               MD_READ_SECTOR:
00DA1C  3  20 25 DA                     JSR MD_READ_RAW_SECTOR
00DA1F  3  20 B6 DA                     JSR DEBSECR256
00DA22  3  A9 00                        LDA #$00                        ; SHOULD ALWAYS SUCCEED
00DA24  3  60                           RTS
00DA25  3               
00DA25  3               MD_READ_RAW_SECTOR:
00DA25  3               		PRTDBG "MD Read Sector$"
00DA25  3  20 15 CF                     JSR     GET_DRIVE_DEVICE
00DA28  3  29 01        		and 	#$01			; only want drive cfg
00DA2A  3  0A           		asl	a			; SHIFT 6
00DA2B  3  0A           		asl	a			;
00DA2C  3  0A           		asl	a			;
00DA2D  3  0A              		asl	a			;
00DA2E  3  0A           		asl	a			;
00DA2F  3  0A           		asl	a			;
00DA30  3  29 7F        		AND 	#%01111111              ; TOGGLE READ
00DA32  3  AA                           TAX                             ; STASH CONTROL WORD
00DA33  3  20 73 DA                     JSR     MD_CONVERT_SECTOR
00DA36  3  E0 00                        cpx     #$00                    ; read if ram
00DA38  3  F0 0C                        BEQ     :+
00DA3A  3  EE 7E DB                     inc     debcyll                 ; if rom, inc bank by 4 ()
00DA3D  3  EE 7E DB                     inc     debcyll
00DA40  3  EE 7E DB                     inc     debcyll
00DA43  3  EE 7E DB                     inc     debcyll
00DA46  3               :
00DA46  3  AD 7E DB       	        LDA    	debcyll			; GET BANK
00DA49  3  AC 80 DB     		LDY    	debsehd			; GET PAGE
00DA4C  3                               PRTDBG "DO PAGER$"
00DA4C  3  20 00 02                     JSR     MD_PAGERA
00DA4F  3                               PRTDBG "PAGER RETURN$"
