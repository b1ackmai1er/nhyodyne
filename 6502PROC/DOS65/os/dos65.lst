00D068  3               ;	LDA	#$80			;
00D068  3               ;	STA	UART3			; SET DLAB FLAG
00D068  3               ;	LDA	#12			; SET TO 12 = 9600 BAUD
00D068  3               ;	STA	UART0			; save baud rate
00D068  3               ;	LDA	#00			;
00D068  3               ;	STA	UART1			;
00D068  3               ;	LDA	#03			;
00D068  3               ;	STA	UART3			; SET 8 BIT DATA, 1 STOPBIT
00D068  3               ;	STA	UART4			;
