00D069  3               ;__WRSER1________________________________________________________________________________________________________________________
00D069  3               ;
00D069  3               ;	WRITE CHARACTER(A) TO UART
00D069  3               ;________________________________________________________________________________________________________________________________
00D069  3               ;
00D069  3               WRSER1:
00D069  3  48           	PHA
00D06A  3               WRSER1a:
00D06A  3  AD 6D 03     	LDA	UART5			; READ LINE STATUS REGISTER
00D06D  3  29 20        	AND	#$20			; TEST IF UART IS READY TO SEND (BIT 5)
00D06F  3  C9 00        	CMP     #$00
00D071  3  F0 F7        	BEQ	WRSER1a			; NO, WAIT FOR IT
00D073  3  68           	PLA
00D074  3  8D 68 03     	STA	UART0			; THEN WRITE THE CHAR TO UART
00D077  3  60           	RTS
00D078  3               
00D078  3               ;__RDSER1________________________________________________________________________________________________________________________
00D078  3               ;
00D078  3               ;	READ CHARACTER FROM UART TO (A)
00D078  3               ;________________________________________________________________________________________________________________________________
00D078  3               ;
00D078  3               RDSER1:
00D078  3  AD 6D 03     	LDA	UART5			; READ LINE STATUS REGISTER
00D07B  3  29 01        	AND	#$01			; TEST IF DATA IN RECEIVE BUFFER
00D07D  3  C9 00        	CMP 	#$00
00D07F  3  F0 04        	BEQ	RDSER1N			; LOOP UNTIL DATA IS READY
00D081  3  AD 68 03     	LDA	UART0			; THEN READ THE CHAR FROM THE UART
00D084  3  60           	RTS
00D085  3               RDSER1N:
00D085  3  A9 00        	LDA	#$00			;
00D087  3  60           	RTS				;
00D088  3               
00D088  3               ;__RDSER1W_______________________________________________________________________________________________________________________
00D088  3               ;
00D088  3               ;	READ CHARACTER FROM UART TO (A) - WAIT FOR CHAR
00D088  3               ;________________________________________________________________________________________________________________________________
00D088  3               ;
00D088  3               
00D088  3               RDSER1W:
00D088  3  AD 6D 03     		LDA	UART5			; READ LINE STATUS REGISTER
00D08B  3  29 01        		AND	#$01			; TEST IF DATA IN RECEIVE BUFFER
00D08D  3  C9 00        		CMP 	#$00
00D08F  3  F0 F7        		BEQ	RDSER1W			; LOOP UNTIL DATA IS READY
00D091  3  AD 68 03     		LDA	UART0			; THEN READ THE CHAR FROM THE UART
00D094  3  29 7F        		AND #$7F
00D096  3  60           		RTS
00D097  3               
00D097  3               ;__SERIALSTATUS__________________________________________________________________________________________________________________
00D097  3               ;
00D097  3               ;	READ UARD STATUS
00D097  3               ;________________________________________________________________________________________________________________________________
00D097  3               ;
00D097  3               SERIALSTATUS:
00D097  3  AD 6D 03     	LDA	UART5			; READ LINE STATUS REGISTER
00D09A  3  29 01        	AND	#$01			; TEST IF DATA IN RECEIVE BUFFER
00D09C  3  C9 00        	CMP 	#$00
00D09E  3  F0 03        	BEQ	RDSTAT1			; NO, INDICATE NO CHAR
00D0A0  3  A9 FF        	LDA	#$FF			; GET DATA CHAR
00D0A2  3  60           	RTS
00D0A3  3               RDSTAT1:
00D0A3  3  A9 00        	LDA	#$00			; GET DATA CHAR
00D0A5  3  60           	RTS
