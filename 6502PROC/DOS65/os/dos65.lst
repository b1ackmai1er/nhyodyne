00DA50  3  20 15 CF                     JSR     GET_DRIVE_DEVICE
00DA53  3  29 01        		and 	#$01			; only want drive cfg
00DA55  3  C9 00                        CMP     #$00                    ; NO WRITE FOR ROM
00DA57  3  F0 03                        BEQ     MD_WRITE_SECTOR_RAM
00DA59  3  A9 FF                        LDA     #$FF
00DA5B  3  60                           RTS
00DA5C  3               MD_WRITE_SECTOR_RAM:
