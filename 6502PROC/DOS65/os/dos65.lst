00D1D5  3               IDE_IDENTIFY_TYPE:
00D1D5  3  48           	PHA
00D1D6  3  20 05 D4     	JSR	IDE_WAIT_NOT_BUSY	;MAKE SURE DRIVE IS READY
00D1D9  3  B0 40        	BCS 	IDE_IDENTIFY_TYPE_ERROR	; IF TIMEOUT, REPORT ERROR
00D1DB  3  A9 00        	LDA 	#$00
00D1DD  3  8D 80 DB     	STA	debsehd
00D1E0  3  8D 7E DB     	STA	debcyll			; STORE IN TRACK (lsb)
00D1E3  3  8D 7F DB     	STA	debcylm			; STORE IN TRACK (msb)
00D1E6  3  68           	PLA				; GET DRIVE TYPE
00D1E7  3  20 7A D3     	JSR 	IDE_READ_SECTOR_DIRTY1
00D1EA  3  C9 FF        	CMP 	#$FF			; IS THERE A READ ERROR?
00D1EC  3  F0 2D        	BEQ 	IDE_IDENTIFY_TYPE_ERROR
00D1EE  3  AD F7 E5     	LDA 	hstbuf+$01FE
00D1F1  3  C9 55        	CMP 	#$55
00D1F3  3  D0 23        	BNE 	IDE_IDENTIFY_TYPE_OK
00D1F5  3  AD F8 E5     	LDA 	hstbuf+$01FF
00D1F8  3  C9 AA        	CMP 	#$AA
00D1FA  3  D0 1C        	BNE 	IDE_IDENTIFY_TYPE_OK
00D1FC  3  AD BB E5     	LDA 	hstbuf+$01C2
00D1FF  3  C9 00        	CMP 	#$00
00D201  3  D0 18        	BNE 	IDE_IDENTIFY_TYPE_ERROR
00D203  3  AD CB E5     	LDA 	hstbuf+$01D2
00D206  3  C9 00        	CMP 	#$00
00D208  3  D0 11        	BNE 	IDE_IDENTIFY_TYPE_ERROR
00D20A  3  AD DB E5     	LDA 	hstbuf+$01E2
00D20D  3  C9 00        	CMP 	#$00
00D20F  3  D0 0A        	BNE 	IDE_IDENTIFY_TYPE_ERROR
