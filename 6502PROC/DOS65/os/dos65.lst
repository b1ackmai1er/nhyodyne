00D506  3  4A           	LSR	A			;
00D507  3  4A           	LSR	A			; DIVIDE BY 4 (FOR BLOCKING)
00D508  3  29 0F        	AND 	#$0F 			; CLEAR UPPER 4 BITS (JUST 'CAUSE)
