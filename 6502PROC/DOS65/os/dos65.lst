00D955  3               ;  INIT -- Copy code into $0200-$02FF for controling banking and copying
00D955  3               ;____________________________________________________________________________________________________
00D955  3               ;
00D955  3               MD_INIT:
00D955  3                       PRTDBG "MD INIT:$"
00D955  3  48 DA 5A A2          PRTS "MD: UNITS=2 RAMDISK=256KB ROMDISK=384KB$"
00D959  3  00 BD 6E D9  
00D95D  3  E8 C9 24 F0  
00D996  3  20 00 CC            	JSR	NEWLINE
00D999  3               MD_REINIT:
00D999  3  A2 00                LDX     #$00
00D99B  3               :
00D99B  3  BD A7 D9             LDA     md_pagecode,X
00D99E  3  9D 00 02             STA     MD_PAGERA,X
00D9A1  3  E8                   INX
00D9A2  3  E0 00                CPX     #$00
00D9A4  3  D0 F5                BNE     :-
00D9A6  3  60                   RTS
00D9A7  3               
00D9A7  3               ;       X=Control Word
00D9A7  3               ;	7 6 5 4  3 2 1 0
00D9A7  3               ;	^ ^ ^ ^  ^ ^ ^ ^
00D9A7  3               ;       : : X X  X X X X    = UNUSED
00D9A7  3               ;	: :---------------0 = RAM=0, ROM=1
00D9A7  3               ;	:-----------------0 = Read=0, Write=1
00D9A7  3               ;       A= bank
00D9A7  3               ;       Y= page
00D9A7  3               ;
00D9A7  3               md_pagecode:
00D9A7  3  48                   PHA
00D9A8  3  84 15                STY     MD_PAGESE+1     ; setup copy from pointer
00D9AA  3  A0 00                LDY     #$00
00D9AC  3  84 14                STY     MD_PAGESE
00D9AE  3  8A                   TXA
00D9AF  3  29 80                AND     #%10000000
00D9B1  3  C9 00                CMP     #$00
00D9B3  3  D0 38                BNE     MD_PAGE_WRITE
00D9B5  3               ; PERFORM READ HERE
00D9B5  3  8A                   TXA
00D9B6  3  29 40                AND     #%01000000
00D9B8  3  C9 00                CMP     #$00
00D9BA  3  D0 0D                BNE     MD_PAGE_ROREAD
00D9BC  3               ; DO RAM READ
00D9BC  3  A9 80                LDA     #$80
00D9BE  3  8D 7C 03             STA     MPCL_ROM
00D9C1  3  68                   PLA
00D9C2  3  09 80                ORA     #$80
00D9C4  3  8D 78 03             STA     MPCL_RAM
00D9C7  3  D0 0B                BNE     MD_PAGE_COPYFRM
00D9C9  3               MD_PAGE_ROREAD:
00D9C9  3  A9 00                LDA     #$00
00D9CB  3  8D 78 03             STA     MPCL_RAM
00D9CE  3  68                   PLA
00D9CF  3  29 7F                AND     #$7F
00D9D1  3  8D 7C 03             STA     MPCL_ROM
00D9D4  3               MD_PAGE_COPYFRM:
00D9D4  3               ; DO THE COPY
00D9D4  3  A2 00                LDX     #$00
00D9D6  3               :
00D9D6  3  DA                   PHX
00D9D7  3  7A                   PLY
00D9D8  3  B1 14                LDA     (MD_PAGESE),Y
00D9DA  3  9D 00 04             STA     MD_PAGEBU,X
00D9DD  3  E8                   INX
00D9DE  3  E0 00                CPX     #$00
00D9E0  3  D0 F4                BNE     :-
00D9E2  3  A9 80                LDA     #$80
00D9E4  3  8D 7C 03             STA     MPCL_ROM
00D9E7  3  A9 8E                LDA     #$8E
00D9E9  3  8D 78 03             STA     MPCL_RAM
00D9EC  3  60                   RTS
00D9ED  3               MD_PAGE_WRITE:
00D9ED  3  68                   PLA
00D9EE  3  09 80                ORA     #%10000000
00D9F0  3  8D 78 03             STA     MPCL_RAM
00D9F3  3               ; DO THE COPY
00D9F3  3  A2 00                LDX     #$00
00D9F5  3               :
00D9F5  3  DA                   PHX
00D9F6  3  7A                   PLY
00D9F7  3  BD 00 04             LDA     MD_PAGEBU,X
00D9FA  3  91 14                STA     (MD_PAGESE),Y
00D9FC  3  E8                   INX
00D9FD  3  E0 00                CPX     #$00
00D9FF  3  D0 F4                BNE     :-
00DA01  3  A9 80                LDA     #$80
00DA03  3  8D 7C 03             STA     MPCL_ROM
00DA06  3  A9 8E                LDA     #$8E
00DA08  3  8D 78 03             STA     MPCL_RAM
00DA0B  3  60                   RTS
