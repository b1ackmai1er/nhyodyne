00D955  3               
00D955  3               ; RAM BANK $0C is RAM area for Drivers
00D955  3               ; RAM BANK $0E is operating bank for DOS/65 $8000-$FFFF
00D955  3               ; RAM BANK $0F is fixed bank $0000-$7FFF
00D955  3               ;
00D955  3               ; ROM BANKS $00 and $0C-$0F are reserved for ROMWBW code
00D955  3               ;
00D955  3               ; ROM MEMORY PAGE CONFIGURATION LATCH CONTROL PORT
00D955  3               ;       A15 IS INVERTED FOR THE NYHODYNE 65C02 CPU . . .
00D955  3               ;	7 6 5 4  3 2 1 0      APPLICABLE TO THE UPPER MEMORY PAGE $8000-$FFFF
00D955  3               ;	^ ^ ^ ^  ^ ^ ^ ^
00D955  3               ;	: : : :  : : : :--0 = A15 ROM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : : :  : : :----0 = A16 ROM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : : :  : :------0 = A17 ROM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : : :  :--------0 = A18 ROM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : : :-----------0 = A19 ROM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : :-------------0 = A20 ROM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: :---------------0 = ROM BOOT OVERRIDE DEFAULT IS 0
00D955  3               ;	:-----------------0 = LOWER PAGE ROM SELECT (0=ROM, 1=NOTHING) DEFAULT IS 0
00D955  3               ;
00D955  3               ; RAM PAGE CONFIGURATION LATCH CONTROL PORT
00D955  3               ;
00D955  3               ;	7 6 5 4  3 2 1 0      APPLICABLE TO THE UPPER MEMORY PAGE $8000-$FFFF
00D955  3               ;	^ ^ ^ ^  ^ ^ ^ ^
00D955  3               ;	: : : :  : : : :--0 = A15 RAM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : : :  : : :----0 = A16 RAM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : : :  : :------0 = A17 RAM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : : :  :--------0 = A18 RAM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : : :-----------0 = A19 RAM ONLY ADDRESS LINE DEFAULT IS 0
00D955  3               ;	: : :-------------0 = UNDEFINED DEFAULT IS 0
00D955  3               ;	: :---------------0 = RAM BOOT OVERRIDE DEFAULT IS 0
00D955  3               ;	:-----------------0 = LOWER PAGE RAM SELECT (0=NOTHING, 1=RAM) DEFAULT IS 0;
