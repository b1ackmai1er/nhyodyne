00D195  3               ;
00D195  3               ;__PPIDE_PROBE_______________________________________________________________________________________
00D195  3               ;
00D195  3               ;  PROBE FOR IDE HARDWARE
00D195  3               ;____________________________________________________________________________________________________
00D195  3               ;
00D195  3               PPIDE_PROBE:
00D195  3               ;
00D195  3               	; BELOW TESTS FOR EXISTENCE OF AN IDE CONTROLLER ON THE
00D195  3               	; PPIDE INTERFACE.  WE WRITE A VALUE OF ZERO FIRST SO THAT
00D195  3               	; THE PPI BUS HOLD WILL RETURN A VALUE OF ZERO IF THERE IS
00D195  3               	; NOTHING CONNECTED TO PPI PORT A.  THEN WE READ THE STATUS
00D195  3               	; REGISTER.  IF AN IDE CONTROLLER IS THERE, IT SHOULD ALWAYS
00D195  3               	; RETURN SOMETHING OTHER THAN ZERO.  IF AN IDE CONTROLLER IS
00D195  3               	; THERE, THEN THE VALUE WRITTEN TO PPI PORT A IS IGNORED
00D195  3               	; BECAUSE THE WRITE SIGNAL IS NEVER PULSED.
00D195  3               
00D195  3  A9 00        	LDA 	#$00
00D197  3  8D 60 03     	STA 	PPIDELO 		; PPI PORT A, DATALO
00D19A  3               
00D19A  3  20 05 D4     	JSR	IDE_WAIT_NOT_BUSY	; WAIT FOR BUSY TO CLEAR
00D19D  3  B0 34        	BCS 	PPIDE_PROBE_FAIL	; IF TIMEOUT, REPORT NO IDE PRESENT
00D19F  3  A9 0F        	LDA	#PPIDE_STATUS		; GET STATUS
00D1A1  3  20 B4 D5     	JSR	IDE_READ
00D1A4  3  8A           	TXA
00D1A5  3  29 40        	AND 	#%01000000
00D1A7  3  C9 00        	CMP 	#$00
00D1A9  3  F0 28        	BEQ 	PPIDE_PROBE_FAIL	; IF NOT RDY BIT (BIT 6) THEN REPORT NO IDE PRESENT
00D1AB  3               
00D1AB  3               	; CHECK SIGNATURE
00D1AB  3  A9 0A        	LDA	#PPIDE_SEC_CNT
00D1AD  3  20 B4 D5     	JSR	IDE_READ
00D1B0  3  E0 01        	CPX 	#$01
00D1B2  3  D0 1F        	BNE 	PPIDE_PROBE_FAIL	; IF not '01' THEN REPORT NO IDE PRESENT
00D1B4  3  A9 0B        	LDA	#PPIDE_LBALOW
00D1B6  3  20 B4 D5     	JSR	IDE_READ
00D1B9  3  E0 01        	CPX 	#$01
00D1BB  3  D0 16        	BNE 	PPIDE_PROBE_FAIL	; IF not '01' THEN REPORT NO IDE PRESENT
00D1BD  3  A9 0C        	LDA	#PPIDE_LBAMID
00D1BF  3  20 B4 D5     	JSR	IDE_READ
00D1C2  3  E0 00        	CPX 	#$00
00D1C4  3  D0 0D        	BNE 	PPIDE_PROBE_FAIL	; IF not '00' THEN REPORT NO IDE PRESENT
00D1C6  3  A9 0D        	LDA	#PPIDE_LBAHI
00D1C8  3  20 B4 D5     	JSR	IDE_READ
00D1CB  3  E0 00        	CPX 	#$00
00D1CD  3  D0 04        	BNE 	PPIDE_PROBE_FAIL	; IF not '00' THEN REPORT NO IDE PRESENT
00D1CF  3  18           	CLC
00D1D0  3  4C D4 D1     	JMP 	PPIDE_PROBE_SUCCESS
00D1D3  3               PPIDE_PROBE_FAIL:
00D1D3  3  38           	SEC
00D1D4  3               PPIDE_PROBE_SUCCESS:
00D1D4  3  60           	RTS				; DONE, NOTE THAT A=0 AND Z IS SET
00D1D5  3               
00D1D5  3               
00D1D5  3               ;___IDE_IDENTIFY_TYPE____________________________________________________________________________________
00D1D5  3               ;
00D1D5  3               ; 	READ THE DISK TYPE AND DETERMINE IF IT IS USABLE BY DOS/65
00D1D5  3               ; 	A=IDE DEVICE (0=MST,1=SLV)
00D1D5  3               ;________________________________________________________________________________________________________
