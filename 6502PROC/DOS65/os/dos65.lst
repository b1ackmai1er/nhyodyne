00DA5F  3  20 73 DA                     JSR     MD_CONVERT_SECTOR
00DA62  3  20 DC DA                     JSR     BLKSECR256
00DA65  3  AD 7E DB      	        LDA    	debcyll			; GET BANK
00DA68  3  AC 80 DB     		LDY    	debsehd			; GET PAGE
00DA6B  3  A2 80        		LDX 	#$80                    ; TOGGLE WRITE RAM
00DA6D  3                               PRTDBG "DO PAGER$"
